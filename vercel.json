{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/**/*.{js,jsx,ts,tsx}",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/pages/$1"
    }
  ]
}
